			jQuery(\'#menu-product-tour\').removeClass(\'stick\');
		}
	}

	function sticky_relocate_doc() {
		var window_top 		= jQuery(window).scrollTop();
		var menu_top 		= jQuery(\'#sidebar .wpb_accordion\').offset().top - 80;
		var menu_bottom 	= menu_top + jQuery(\'#docs-sidebar\').height();
		var content_bottom 	= jQuery(\'#content\').offset().top - window_top + jQuery(\'#content\').height() + 80;

		var menu_height = jQuery(\'#docs-sidebar\').height();
		var conent_height = jQuery(\'#content\').height();
		
		//console.log(content_bottom + " - " + menu_bottom);

		if ( menu_height < conent_height ){
			if (window_top > menu_top && content_bottom > menu_bottom) {
				jQuery(\'#docs-sidebar\').removeClass(\'stick-bottom\').addClass(\'stick\');
			} else 
				if ( content_bottom <= menu_bottom ) {
					jQuery(\'#docs-sidebar\').removeClass(\'stick\').addClass(\'stick-bottom\');
				} else {
					jQuery(\'#docs-sidebar\').removeClass(\'stick\').removeClass(\'stick-bottom\');
				}
		} else {
			jQuery(\'#docs-sidebar\').removeClass(\'stick\').removeClass(\'stick-bottom\');
		}
	}

	jQuery(function () {
		var menu = jQuery(\'#menu-product-tour\');

		if ( menu.length > 0 ) {
			jQuery(window).scroll(sticky_relocate);
			sticky_relocate();
		}

		var menu_doc = jQuery(\'#docs-sidebar\');

		if ( menu_doc.length > 0 ) {
			jQuery(window).scroll(sticky_relocate_doc);
			jQuery("#docs-sidebar").on( "accordionactivate", sticky_relocate_doc);
			sticky_relocate_doc();
		}
	});
</script>


